{
  "name": "fit-committed-stock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "gen-api": "openapi-generator-cli generate -i https://shopify-data.it.fantastiskefroe.dk/q/openapi -g typescript-rxjs -o ./src/api/shopify-data",
    "build": "run-p gen-api && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "rxjs": "^7.8.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "typescript": "~4.9.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
